{
	"compilerOptions": {
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"skipLibCheck": false,
		"esModuleInterop": true,
		"strict": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"jsx": "react",
		"target": "ES6",
		"module": "ES6"
	},
	"include": ["src", "vite"]
}
